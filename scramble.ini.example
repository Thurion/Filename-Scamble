[General]
Input = F:\temp\test\in
Output = F:\temp\test\out

# Use salt when hashing file names
# Possible values: {yes, no}
Use salt = yes

# store copy of unencrypted mapping file in same directory as this ini?
Store copy of mapping = yes

[External Program]
# launch external program after scrambling files
Launch program = no

Program = C:\WINDOWS\system32\cmd.exe
Parameters = /c echo "hello world"

# timeout in seconds, 0 = no timeout
Timeout = 3600

[Logging]
Enable = yes

# Use an absolute path or a relative to the working directory
File = log.log

# Log levels are: CRITICAL, ERROR, WARNING, INFO, DEBUG, NOTSET
Level = DEBUG

[Encryption]
Password = secret
